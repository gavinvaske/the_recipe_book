{
  "name": "nodejs_starter_template",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "version": "1.0.0",
  "description": "An application used to get up and running with a new nodejs project with a linter, test library and basic configuration already setup",
  "type": "module",
  "targets": {
    "main": false,
    "resources": {
      "source": "application/react/index.jsx",
      "distDir": "build",
      "publicUrl": "build"
    }
  },
  "scripts": {
    "build": "parcel build --no-cache",
    "start": "npm run build && npx tsx application/api/index.ts",
    "react": "npm run parcel:remove-cache && parcel watch",
    "test": "jest",
    "cypress:run": "cypress run",
    "acceptance": "start-server-and-test start http://localhost:8080 cypress:run",
    "lint": "eslint .",
    "fix-lint": "eslint . --fix",
    "verify": "npm run test && npm run lint",
    "parcel:remove-cache": "rm -rf .parcel-cache"
  },
  "config": {
    "mongodbMemoryServer": {}
  },
  "author": "Gavin Vaske",
  "license": "ISC",
  "dependencies": {
    "@parcel/transformer-sass": "2.13.3",
    "@sendgrid/mail": "8.1.3",
    "@tanstack/react-query": "5.63.0",
    "@tanstack/react-table": "8.20.6",
    "aws-sdk": "2.1692.0",
    "axios": "1.7.9",
    "bcrypt": "5.1.1",
    "bcryptjs": "2.4.3",
    "buffer": "6.0.3",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.7",
    "decimal.js": "10.4.3",
    "dotenv": "16.4.5",
    "ejs": "3.1.10",
    "express": "4.21.2",
    "express-ejs-layouts": "2.5.1",
    "express-session": "1.18.1",
    "form-data": "4.0.0",
    "js-search": "2.0.1",
    "jsonwebtoken": "9.0.2",
    "lodash.isnil": "4.0.0",
    "mime": "3.0.0",
    "mobx": "6.13.5",
    "mobx-react-lite": "4.1.0",
    "mongodb-memory-server": "9.5.0",
    "mongoose": "8.10.1",
    "mongoose-delete": "1.0.2",
    "multer": "1.4.5-lts.1",
    "parcel": "2.13.3",
    "process": "0.11.10",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.54.2",
    "react-icons": "5.4.0",
    "react-router-dom": "6.28.1",
    "sass": "1.78.0",
    "socket.io": "4.8.1",
    "socket.io-client": "4.8.1",
    "uuid": "10.0.0",
    "xml2json": "0.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/eslint-parser": "7.25.1",
    "@babel/preset-env": "7.25.4",
    "@babel/preset-typescript": "7.24.7",
    "@types/express": "4.17.21",
    "@types/js-search": "1.4.4",
    "@types/mime": "4.0.0",
    "@types/mongodb": "4.0.6",
    "@types/mongoose-delete": "1.0.6",
    "@types/multer": "1.4.11",
    "@types/node": "22.10.7",
    "@types/react": "18.3.18",
    "@types/uuid": "9.0.8",
    "babel-jest": "29.7.0",
    "browserify-zlib": "0.2.0",
    "chance": "1.1.12",
    "crypto-browserify": "3.12.0",
    "cypress": "13.17.0",
    "eslint": "8.57.0",
    "events": "3.3.0",
    "jest": "29.7.0",
    "jest-when": "3.5.2",
    "path-browserify": "1.0.1",
    "start-server-and-test": "2.0.3",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "ts-node": "10.9.2",
    "tsx": "4.16.2",
    "typescript": "5.7.3"
  }
